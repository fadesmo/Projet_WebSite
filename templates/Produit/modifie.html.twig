{% extends 'Layouts/layout_vente.html.twig' %}

{% block title %}{{ parent() }} : Modification{% endblock %}

{% block titre_principal %}{{ parent() }} : Modification de produit{% endblock %}

{% block vue %}
    <h2>Modification du produit : {{ produit.libelle }}</h2>

    {{ form_start(myform) }}

    {{ form_errors(myform) }}

    {{ form_row(myform.libelle) }}


    {{ form_rest(myform) }}

    {{ form_end(myform) }}


    <h2>dump</h2>
    {{ dump(myform) }}
{% endblock %}